<div class="table-responsive-summary">
    <table class="table table-striped" >
      <thead class="thead">
        <tr>
          <th>
            <p data-target="orders.totalOrders"><strong>Total pedidos: <%= @total_orders %></strong></p>
          </th>
          <th>Normal</th>
          <th>Low Carb</th>
          <th>High Carb</th>
          <th>High Protein</th>
          <th>Keto</th>
          <th>High Protein/Low Carb</th>
          <th>High Protein/High Carb</th>
          <th>Extra Veggie</th>
        </tr>
      </thead>
      <tbody data-target="orders.mealsSummary">
        <% @meals_summary.each do |e| %>
          <tr>
          <th><%= e[0] %></th>
          <td><%= e[1] %></td>
          <td><%= e[2] %></td>
          <td><%= e[3] %></td>
          <td><%= e[4] %></td>
          <td><%= e[5] %></td>
          <td><%= e[6] %></td>
          <td><%= e[7] %></td>
          <td><%= e[8] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>
